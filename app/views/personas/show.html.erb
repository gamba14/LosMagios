<h1><%= @persona.nombre + @persona.apellido %></h1>

<%= link_to "nuevo tramite", new_persona_tramite_path(@persona) %>


<table class="table table-hover">
<thead>
	<tr>
		<th>Tipo</th>
		<th>Descripcion</th>
		<th>Id</th>
		<th>Estado</th>
	</tr>
</thead>
<tbody>
	<% @persona.tramites.each do |t| %>
	<tr>		
		<td><%= t.tipo %></td>
		<td><%= t.comentario %></td>
		<td><%= t.id %></td>
		<td><%= t.estado %></td>
		<% end %>
	</tr>	
</tbody>
</table>
